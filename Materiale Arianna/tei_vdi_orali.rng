            <choice>
               <ref name="tei_model.descLike"/>
               <ref name="tei_model.certLike"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="tei_vdi_orali-join-joinTargets3-constraint-rule-49">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      context="tei:join">
               <sch:assert test="contains( normalize-space( @target ),' ')">
          You must supply at least two values for @target on <sch:name/>
               </sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tei_att.global.attributes"/>
         <ref name="tei_att.pointing.attributes"/>
